<?xml version="1.0" encoding="ISO-8859-1"?>
<aiml>
<!-- basic_chat.aiml 
    This AIML file just goes over Greetings, and asking and replies of the user's wellbeing (how are you, etc)
-->

    <category>
        <pattern>HELLO</pattern>
        <template>
        	<random>
            	<li> Hello! </li>
            	<li> Hi! Nice to meet you! </li>
            	<li> Hey! </li>
            	<li> Greetings! Please ask me anything about Middlesex Mauritius </li>
            	<li> Hey hey! How can I help you today? </li>
         	</random>
     	</template>
    </category>
    
	<category>	
        <pattern>HI</pattern>
        <template>
            <srai>HELLO</srai>
        </template>
    </category>

	<category>	
        <pattern>GREETINGS</pattern>
        <template>
            <srai>HELLO</srai>
        </template>
    </category>

